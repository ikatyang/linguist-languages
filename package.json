{
  "name": "linguist-languages",
  "type": "module",
  "version": "7.21.0",
  "description": "Linguist languages.yml in JSON format",
  "keywords": [
    "data",
    "json",
    "language",
    "linguist"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./data/*": "./data/*.js"
  },
  "repository": "https://github.com/ikatyang/linguist-languages",
  "homepage": "https://github.com/ikatyang/linguist-languages#readme",
  "author": {
    "name": "Ika",
    "email": "ikatyang@gmail.com",
    "url": "https://github.com/ikatyang"
  },
  "license": "MIT",
  "scripts": {
    "prepack": "pnpm run build",
    "lint": "prettier --check .",
    "check": "tsc --noEmit",
    "test": "vitest",
    "build": "vite-node ./scripts/generate.ts -- run"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "18.16.19",
    "@vitest/coverage-v8": "0.33.0",
    "camelcase": "7.0.1",
    "del": "7.0.0",
    "jest-snapshot-serializer-raw": "1.2.0",
    "make-dir": "4.0.0",
    "prettier": "3.0.0",
    "typescript": "5.1.6",
    "vite": "4.4.2",
    "vite-node": "0.33.0",
    "vitest": "0.33.0",
    "yaml": "2.3.1"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/lib/**/*",
    "/data/**/*",
    "/ThirdPartyNoticeText.txt"
  ],
  "packageManager": "pnpm@8.6.6"
}
